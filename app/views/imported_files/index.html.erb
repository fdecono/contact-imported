<h1>Imported Files</h1>

<div class="media">
  <div class="media-body">
    <table class="table table-hover">
      <thead>
        <tr>
          <th>File Name</th>
          <th>Status</th>
          <th></th>
          <th></th>
        </tr>
      </thead>

      <tbody>
        <% @imported_files.each do |imported_file| %>
          <tr>
            <td><%= imported_file.file_name %></td>
            <td><%= imported_file.status %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
<br>
<%= button_to 'Contacts', contacts_path, method: :get %>
<br>
<%= button_to 'Error Logs', error_logs_path, method: :get %>
